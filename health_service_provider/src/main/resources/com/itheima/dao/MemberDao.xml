<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itheima.dao.MemberDao">
    <select id="findByTelephone" resultType="com.itheima.pojo.Member">
        select * from t_member where phoneNumber=#{telephone}
    </select>
    <insert id="add" parameterType="com.itheima.pojo.Member">
        insert into t_member value (null, #{fileNumber},#{name},#{sex},#{idCard},#{phoneNumber},#{regTime},#{password},#{email},#{birthday},#{remark})
    </insert>
</mapper>